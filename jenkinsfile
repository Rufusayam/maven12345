pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload_loans')
        {
            steps
            {
                git 'https://github.com/Rufusayam/maven12345.git'
            }
        }
        stage('ContinuousBuild_loans')
        {
            steps
            {
                sh 'mvn package'
       }
    }
}
